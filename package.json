{
  "name": "secure-logger-nest",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "imports": {"#app/*": "./dist/*.js"},
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "run-p tsc:watch serve:watch",
    "tsc:watch": "tsc -p tsconfig.build.json -w",
    "serve:watch": "node --watch dist/main.js",
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "pino": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "remeda": "^2.30.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8"
  }
}
